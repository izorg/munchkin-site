{
  "name": "munchkin-site",
  "homepage": "https://allmunchkins.com",
  "repository": "https://github.com/izorg/munchkin-site",
  "scripts": {
    "build": "gatsby build && cp CNAME public/",
    "dev": "gatsby develop --host=localhost",
    "i18n:download": "node scripts/i18n/download.js",
    "i18n:translate": "node scripts/i18n/translate.js",
    "i18n:upload": "node scripts/i18n/upload.js",
    "lint": "eslint . --ext .js,.jsx",
    "serve": "gatsby serve"
  },
  "browserslist": [
    "android >= 4.4.2",
    "chrome >= 49",
    "edge >= 14",
    "firefox >= 45",
    "ie >= 11",
    "safari >= 10"
